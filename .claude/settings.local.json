{
  "permissions": {
    "allow": [
      "WebFetch(domain:github.com)",
      "WebFetch(domain:blog.fsck.com)",
      "Bash(chmod:*)",
      "Bash(gh repo create:*)",
      "Bash(git commit:*)",
      "Bash(git push:*)",
      "Bash(AI_AUGMENTED_SOLO_DEV_FRAMEWORK.md)",
      "Bash(git add:*)",
      "Bash(claude-code doctor:*)",
      "Bash(for hook in .claude/hooks/*.sh)",
      "Bash(do if [ -f \"$hook\" ])",
      "Bash(then echo \"Checking $hook:\")",
      "Bash(if [ -x \"$hook\" ])",
      "Bash(then echo \"  ✓ Executable\")",
      "Bash(else echo \"  ✗ Not executable\")",
      "Bash(fi)",
      "Bash(else echo \"  ✗ Missing\")",
      "Bash(done)"
    ],
    "deny": [],
    "ask": []
  }
}
